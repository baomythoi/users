{
  "name": "users",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node ./acme/src/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' -r ts-node ./src/index.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "migrate": "ts-node -r tsconfig-paths/register ./node_modules/knex/bin/cli.js migrate:latest --knexfile knexfile.ts --env development",
    "rollback": "ts-node -r tsconfig-paths/register ./node_modules/knex/bin/cli.js migrate:rollback --knexfile knexfile.ts --env development",
    "migrate:reset": "ts-node -r tsconfig-paths/register ./node_modules/knex/bin/cli.js migrate:rollback --all --knexfile knexfile.ts --env development && ts-node -r tsconfig-paths/register ./node_modules/knex/bin/cli.js migrate:latest --knexfile knexfile.ts --env development"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/compress": "^6.4.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/jwt": "^7.0.0",
    "@fastify/rate-limit": "^8.0.1",
    "@fastify/static": "^6.10.2",
    "@types/bcrypt": "^5.0.2",
    "@types/moment": "^2.13.0",
    "ajv": "^8.16.0",
    "amqplib": "^0.10.3",
    "axios": "1.7.3",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.18.0",
    "form-data": "^4.0.2",
    "joi": "^17.9.2",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "nanoid": "3.3.4",
    "objection": "^3.1.4",
    "pg": "^8.11.5",
    "pino": "^8.14.1",
    "pino-pretty": "^11.2.1",
    "redis": "^4.7.0",
    "sharp": "^0.34.1",
    "tsc-alias": "^1.8.10"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
